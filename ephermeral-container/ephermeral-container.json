{
    "apiVersion": "v1",
    "kind": "EphemeralContainers",
    "metadata": {
        "name": "example-pod"
    },
    "ephemeralContainers": [{
        "command": [
            "sh"
        ],
        "image": "openjdk:11",
        "imagePullPolicy": "IfNotPresent",
        "name": "openjdk",
        "stdin": true,
        "tty": true,
        "terminationMessagePolicy": "File"
    }]
}
